mapping_to_entrez <- function(gene_names) {
  entrez_id <- AnnotationDbi::select(org.Hs.eg.db::org.Hs.eg.db, gene_names, keytype = "SYMBOL", columns=c("ENTREZID", "UNIPROT"))$ENTREZID
